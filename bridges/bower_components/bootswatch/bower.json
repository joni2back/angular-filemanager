{
  "name": "bootswatch",
  "description": "Bootswatch is a collection of themes for Bootstrap.",
  "author": "Thomas Park",
  "homepage": "http://bootswatch.com",
  "repository": {
    "type": "git",
    "url": "git://github.com/thomaspark/bootswatch.git"
  },
  "license": "MIT",
  "ignore": [
    "2",
    "api",
    "assets",
    "bower_components",
    "custom",
    "default",
    "global",
    "help",
    "tests",
    "_config.yml",
    ".gitignore",
    ".travis.yml",
