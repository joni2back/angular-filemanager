    "Gemfile",
    "Gruntfile.js",
    "index.html",
    "package.json",
    "*/index.html"
  ],
  "dependencies": {
    "bootstrap": "~3.3.6"
  },
  "devDependencies": {
    "bootstrap-sass-official": "~3.3.6",
    "font-awesome": "~4.0.0",
    "html5shiv": "~3.7.0",
    "respond": "~1.4.2"
  }
